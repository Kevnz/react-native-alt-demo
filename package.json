{
  "name": "RNAltExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/react-native-webpack-server start",
    "test": "jest --verbose",
    "webpack": "webpack --config webpack.config.js --colors --progress"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/react-native/jestSupport/scriptPreprocess.js",
    "setupEnvScriptFile": "<rootDir>/node_modules/react-native/jestSupport/env.js",
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "source-map",
      "node_modules",
      "react",
      "alt",
      "stores",
      "actions",
      "utils"
    ]
  },
  "dependencies": {
    "alt": "^0.16.9",
    "react-native": "0.6.0-rc",
    "react-native-navbar": "^0.5.0"
  },
  "devDependencies": {
    "anybar-webpack": "^1.0.7",
    "babel": "^5.6.4",
    "babel-core": "^5.1.8",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.1.8",
    "eslint": "^0.19.0",
    "eslint-plugin-babel": "^1.0.0",
    "eslint-plugin-react": "^2.1.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^0.13.2",
    "gulp-watch": "^4.2.4",
    "webpack": "^1.8.4"
  }
}
